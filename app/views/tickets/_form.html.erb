<div class="textarea">
<%= form_for([@project, @ticket], html: { multipart: true } ) do |f| %>
<p>
<%= render 'shared/error_messages', object: f.object %>
<%= f.label(:title) %>
<%= f.text_field(:title) %>

<%= f.label(:description) %>
<%= f.text_area(:description) %>
</p>
<p>
<%= f.label(:asset, "File") %>
<%= f.file_field(:asset) %>
</p>
<%= f.submit %>
<% end %>
</div>